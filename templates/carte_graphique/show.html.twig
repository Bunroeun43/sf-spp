{% extends 'base.html.twig' %}

{% block title %}Carte Graphique
{% endblock %}

{% block body %}
	<div class="page cartegraphique">
		<h1>Carte Graphique</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ carte_graphique.id }}</td>
				</tr>
				<tr>
					<th>Modele</th>
					<td>{{ carte_graphique.modele }}</td>
				</tr>
				<tr>
					<th>Memoire</th>
					<td>{{ carte_graphique.memoire }}</td>
				</tr>
				<tr>
					<th>Longueur</th>
					<td>{{ carte_graphique.longueur }}</td>
				</tr>
				<tr>
					<th>Photo</th>
					<td>{{ carte_graphique.photo }}</td>
				</tr>
				<tr>
					<th>Prix</th>
					<td>{{ carte_graphique.prix }}</td>
				</tr>
				<tr>
					<th>IsActive</th>
					<td>{{ carte_graphique.isActive ? 'Yes' : 'No' }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_carte_graphique_index') }}" class="btn btn-primary btn-crud">retour Ã  la liste</a>

		<a href="{{ path('app_carte_graphique_edit', {'id': carte_graphique.id}) }}" class="btn btn-warning btn-crud">modifier</a>

		{{ include('carte_graphique/_delete_form.html.twig') }}
	</div>

{% endblock %}
